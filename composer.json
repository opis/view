{
    "name": "opis/view",
    "description": "A rendering engine that can be integrated with multiple template engines simultaneously",
    "keywords": ["view", "template", "renderer", "serializable", "engine"],
    "homepage": "http://opis.io/view",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Marius Sarca",
            "email": "marius.sarca@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "opis/routing": "5.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "5.4.*"
    },
    "autoload": {
        "psr-4": {
            "Opis\\View\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Opis\\View\\Test\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "5.0.x-dev"
        }
    }
}
